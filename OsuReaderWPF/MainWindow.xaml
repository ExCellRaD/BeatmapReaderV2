<Window x:Class="OsuReaderWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Content="Go" HorizontalAlignment="Stretch" Margin="10,10,10,10" VerticalAlignment="Stretch" Click="Button_Click" Grid.Column="1"/>
        <TextBox x:Name="txbPad" HorizontalAlignment="Stretch"  Margin="10,30,10.2,10" TextWrapping="Wrap" Text="pad" VerticalAlignment="Stretch"/>
        <TextBox x:Name="txbOutput" HorizontalAlignment="Stretch" Margin="10,10,10,10" TextWrapping="Wrap" Text="Output" VerticalAlignment="Stretch" Grid.ColumnSpan="2" Grid.Row="1" VerticalScrollBarVisibility="Auto" IsReadOnly="True"/>

    </Grid>
</Window>
